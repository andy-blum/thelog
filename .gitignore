# Ignore package-manager files
web/*.*
web/core
web/modules/contrib
web/profiles/contrib
web/themes/contrib
web/sites/default*.*
web/sites/example*.*
web/sites/default
web/sites/*/settings*.php

vendor/

node_modules


# Ignore non-root readmes
readme.*
!/readme.*


# Ignore non-root gitignores
.gitignore
!/.gitignore

# Ignore sensitive info
.env
*.env
error_log

# Ignore paths that contain user-generated content.
sites/*/files
sites/*/private
/.DS_Store
